<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.interswitchng.interswitchpos">

    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:name="com.interswitchng.interswitchpos.POSApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo_image"
        android:label="Smart POS APP"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name="com.interswitchng.interswitchpos.views.activities.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.interswitchng.interswitchpos.views.fragments.TerminalSettingFragment"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
    </application>

</manifest>